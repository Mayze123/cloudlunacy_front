{
  "name": "cloudlunacy_front",
  "version": "1.0.0",
  "description": "A front server for dynamically managing HAProxy routes for MongoDB Docker instances.",
  "main": "server.js",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:docker": "cd .. && bash start-dev.sh",
    "dev:docker:detached": "cd .. && bash start-dev.sh -d",
    "dev:docker:down": "cd .. && bash dev-down.sh",
    "cleanup:scripts": "node scripts/cleanup-shell-scripts.js",
    "certs:renew": "node scripts/renew-letsencrypt.js",
    "certs:renew:force": "node scripts/renew-letsencrypt.js --force"
  },
  "dependencies": {
    "acme-client": "^5.0.0",
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "cloudflare": "^2.9.1",
    "dockerode": "^3.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.7.0",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "node-forge": "^1.3.1",
    "proper-lockfile": "^4.1.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "globals": {
      "process": "readonly",
      "Buffer": "readonly",
      "__dirname": "readonly"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_|^err|^next"
        }
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.0"
  }
}
